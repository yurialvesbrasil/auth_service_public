{
   "name": "typescript-api",
   "version": "1.0.0",
   "description": "Complete API built with Node.js and Typescript",
   "main": "index.js",
   "scripts": {
      "build": "tsc",
      "migration:run:prod": "yarn build && ts-node --transpile-only ./node_modules/typeorm/cli.js -c prod-connection migration:run",
      "seed:run:prod": "ts-node ./node_modules/typeorm-seeding/dist/cli.js -c prod-connection seed",
      "migration:run": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run",
      "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
      "start:prod": "export NODE_ENV=production&& yarn build && node ./dist/src/index.js",
      "start": "node dist/src/index.js",
      "start:dev": "export NODE_ENV=development&& ts-node-dev --transpile-only src/index.ts",
      "test": "yarn test:unit  && yarn test:functional ",
      "lint": "eslint ./src ./test --ext .ts",
      "lint:fix": "eslint ./src ./test --ext .ts --fix",
      "test:functional": "set NODE_ENV=test&& yarn lint && yarn style:check && dotenv -e .env -- jest --projects ./test --runInBand --passWithNoTests",
      "test:unit": "set NODE_ENV=test&& yarn lint && yarn style:check && dotenv -e .env -- jest --passWithNoTests",
      "style:check": "prettier --check ./src/**/*.ts ./test/**/*.ts",
      "style:fix": "prettier --write ./src/**/*.ts ./test/**/*.ts"
   },
   "repository": {
      "type": "git",
      "url": "git@github.com:yurialvesbrasil/perfil-service.git"
   },
   "keywords": [
      "node",
      "nodejs",
      "typescript",
      "api"
   ],
   "author": "Yuri Brasil",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/yurialvesbrasil/perfil-service/issues"
   },
   "homepage": "https://github.com/yurialvesbrasil/perfil-service#readme",
